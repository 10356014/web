<!DOCTYPE html>
<html lang="en">
  <head>
    <meta name="description" content="Vali is a responsive and free admin theme built with Bootstrap 4, SASS and PUG.js. It's fully customizable and modular.">
    <!-- Twitter meta-->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:site" content="@pratikborsadiya">
    <meta property="twitter:creator" content="@pratikborsadiya">
    <!-- Open Graph Meta-->
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="Vali Admin">
    <meta property="og:title" content="Vali - Free Bootstrap 4 admin theme">
    <meta property="og:url" content="http://pratikborsadiya.in/blog/vali-admin">
    <meta property="og:image" content="http://pratikborsadiya.in/blog/vali-admin/hero-social.png">
    <meta property="og:description" content="Vali is a responsive and free admin theme built with Bootstrap 4, SASS and PUG.js. It's fully customizable and modular.">
    <title>CQ2後台管理系統</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Main CSS-->
    <link rel="stylesheet" type="text/css" href="css/main.css">
    <!-- Font-icon css-->
    <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  </head>
  <body class="app sidebar-mini rtl">
    <!-- Navbar-->
    <header class="app-header"><a class="app-header__logo" href="/">CQ2</a>
      <!-- Sidebar toggle button--><a class="app-sidebar__toggle" href="#" data-toggle="sidebar" aria-label="Hide Sidebar"></a>
      <!-- Navbar Right Menu-->
      <ul class="app-nav">
        <!--Notification Menu-->
        <!-- User Menu-->
        <li class="dropdown"><a class="app-nav__item" href="#" data-toggle="dropdown" aria-label="Open Profile Menu"><i class="fa fa-user fa-lg"></i></a>
          <ul class="dropdown-menu settings-menu dropdown-menu-right">
            <li><a class="dropdown-item" href="page-user.html"><i class="fa fa-cog fa-lg"></i> Settings</a></li>
            <li><a class="dropdown-item" href="page-user.html"><i class="fa fa-user fa-lg"></i> Profile</a></li>
            <li><a class="dropdown-item" href="page-login.html"><i class="fa fa-sign-out fa-lg"></i> Logout</a></li>
          </ul>
        </li>
      </ul>
    </header>
    <!-- Sidebar menu-->
    <div class="app-sidebar__overlay" data-toggle="sidebar"></div>
    <aside class="app-sidebar">
      
      <ul class="app-menu">
        <!--左邊欄位開始-->
        <!--首頁-->
        <li><a class="app-menu__item" href="/"><i class="app-menu__icon fa fa-home"></i><span class="app-menu__label">首頁</span></a></li>
        <!--管理店鋪-->
        <li class="treeview"><a class="app-menu__item" href="#" data-toggle="treeview"><i class="app-menu__icon fa fa-th-list"></i><span class="app-menu__label">管理店鋪</span><i class="treeview-indicator fa fa-angle-right"></i></a>
          <ul class="treeview-menu">
            <li><a class="treeview-item" href="/storeListByPage"><i class="icon fa fa-circle-o"></i> 管理店鋪</a></li>
            <li><a class="treeview-item" href="/storeAddForm"><i class="icon fa fa-circle-o"></i> 新增店鋪</a></li>
          </ul>
        </li>

        <!--管理理髮師-->
        <li class="treeview is-expanded"><a class="app-menu__item" href="#" data-toggle="treeview"><i class="app-menu__icon fa fa-th-list"></i><span class="app-menu__label">管理理髮師</span><i class="treeview-indicator fa fa-angle-right"></i></a>
          <ul class="treeview-menu">
              <li><a class="treeview-item active" href="/designerListByPage"><i class="icon fa fa-circle-o"></i> 管理理髮師</a></li>
              <li><a class="treeview-item" href="/designerAddForm"><i class="icon fa fa-circle-o"></i> 新增理髮師</a></li>
          </ul>
        </li>

        <!--管理機器人-->
        <li class="treeview"><a class="app-menu__item" href="#" data-toggle="treeview"><i class="app-menu__icon fa fa-th-list"></i><span class="app-menu__label">管理機器人</span><i class="treeview-indicator fa fa-angle-right"></i></a>
          <ul class="treeview-menu">
              <li><a class="treeview-item" href="/robotListByPage"><i class="icon fa fa-circle-o"></i> 管理機器人</a></li>
              <li><a class="treeview-item" href="/robotAddForm"><i class="icon fa fa-circle-o"></i> 新增機器人</a></li>
          </ul>
        </li>

        <!--管理打卡資料-->
        <li class="treeview"><a class="app-menu__item" href="#" data-toggle="treeview"><i class="app-menu__icon fa fa-th-list"></i><span class="app-menu__label">管理打卡資料</span><i class="treeview-indicator fa fa-angle-right"></i></a>
          <ul class="treeview-menu">
              <li><a class="treeview-item" href="/attendanceListByPage"><i class="icon fa fa-circle-o"></i> 查詢打卡資料</a></li>
          </ul>
        </li>
        

      </ul> 
      <!--左邊欄位結束-->
    </aside>
    
    <main class="app-content">
      <!--表格開始-->
      <div class="row">
        <div class="col-md-12">
          <div class="tile">
            <div class="tile-body">
              <table class="table table-hover table-bordered" id="sampleTable">
                <thead>
                  <tr>
                    <th>理髮師編號</th>
                    <th>理髮師姓名</th>
                    <th>理髮師ID</th>
                    <th>理髮師生日</th>
                    <th>理髮師地址</th>
                    <th>更改</th>
                    <th>刪除</th>
                  </tr>
                </thead>
                <tbody>
                    <!------------------------------>
                    <!-- 顯示資料內容開始         -->
                    <!------------------------------>                       
                    <% for(var i=0; i<data.length; i++) {%>
                        <tr>
                            <!--
                            <td>
                              <a href="/productOne?no=<%= data[i].desNo %>"><%= data[i].desNo %></a>
                            </td>
                            -->
                            <td>
                              <%= data[i].desNo %></a>
                            </td>
                            <td>
                              <%= data[i].desName %></a>
                            </td>
                            <td>
                              <%= data[i].desID %></a>
                            </td>  
                            <td>
                              <%= moment(data[i].birthday).format("YYYY-MM-DD") %></a>
                            </td>
                            <td>
                              <%= data[i].desAddress %></a>
                            </td>   
                            <td>
                              <form role="form" method="get" action="/designerNewDataForm">
                                <input type="hidden" name="desNo" value="<%=data[i].desNo%>"/>
                                <button class="btn btn-warning" type="submit">更改</button>
                              </form>
                            </td>     
                            <td>
                              <form  onsubmit="return confirm('是否確定刪除?');" role="form" action="/designerDelete">
                                <input type="hidden" name="desNo" value="<%=data[i].desNo%>"/>
                                <button class="btn btn-danger" type="submit">刪除</button>
                              </form>
                            </td>              
                        </tr> 
                    <% } %> 
                    <!------------------------------>
                    <!-- 顯示資料內容結束         -->
                    <!------------------------------>                       
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </main>
    <!-- Essential javascripts for application to work-->
    <script src="js/jquery-3.2.1.min.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/main.js"></script>
    <!-- The javascript plugin to display page loading on top-->
    <script src="js/plugins/pace.min.js"></script>
    <!-- Page specific javascripts-->
    <!-- Data table plugin-->
    <script type="text/javascript" src="js/plugins/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="js/plugins/dataTables.bootstrap.min.js"></script>
    <script type="text/javascript">$('#sampleTable').DataTable();</script>
    <!-- Google analytics script-->
    <script type="text/javascript">
      if(document.location.hostname == 'pratikborsadiya.in') {
      	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      	m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      	})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      	ga('create', 'UA-72504830-1', 'auto');
      	ga('send', 'pageview');
      }
    </script>
  </body>
</html>